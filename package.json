{
  "name": "jira-md-sync",
  "version": "0.1.0",
  "type": "commonjs",
  "description": "A tool to sync Jira issues with local markdown files",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "author": "Louis Lu, NZLouis <nzlouis.com@gmail.com> (https://blog.nzlouis.com)",
  "homepage": "https://nzlouis.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nzlouislu/jira-md-sync.git"
  },
  "bugs": {
    "url": "https://github.com/nzlouislu/jira-md-sync/issues"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "jira-to-md": "dist/src/cli/jira-to-md-cli.js",
    "md-to-jira": "dist/src/cli/md-to-jira-cli.js",
    "jira-sync": "dist/src/cli/sync-cli.js",
    "validate-jira-config": "dist/src/cli/validate-config-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "test": "nyc mocha -r ts-node/register --project tsconfig.test.json \"src/**/__tests__/**/*.test.ts\" \"src/testing/**/*.test.ts\" \"src/utils/__tests__/**/*.test.ts\" \"src/cli/__tests__/**/*.test.ts\"",
    "lint": "prettier --check .",
    "verify": "npm test && npm run build",
    "jira-export": "node dist/src/cli/jira-to-md-cli.js",
    "jira-import": "node dist/src/cli/md-to-jira-cli.js",
    "jira-sync": "node dist/src/cli/sync-cli.js",
    "validate": "node dist/src/cli/validate-config-cli.js"
  },
  "keywords": [
    "jira",
    "markdown",
    "sync",
    "jira-api",
    "project-management",
    "task-management",
    "issue-tracking",
    "cli",
    "typescript",
    "workflow",
    "automation",
    "agile",
    "story",
    "issue",
    "atlassian"
  ],
  "license": "MIT",
  "dependencies": {
    "@octokit/graphql": "^7.0.1",
    "dotenv": "^16.3.1",
    "github-projects-md-sync": "^0.1.8",
    "jira-client": "^8.2.2",
    "jira2md": "^3.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "strip-indent": "^4.1.0",
    "unified": "^11.0.5",
    "unist-util-select": "^5.1.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/debug": "^4.1.12",
    "@types/jira-client": "^7.1.9",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.5",
    "chai": "^6.2.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ]
}